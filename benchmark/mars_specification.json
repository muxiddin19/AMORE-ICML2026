  "evaluation_metrics": {
    "primary": {
      "task_success_rate": {
        "description": "Fraction of tasks where >=80% of subtasks succeed",
        "formula": "successful_tasks / total_tasks"
      },
      "subtask_success_rate": {
        "description": "Fraction of successfully completed subtasks",
        "formula": "successful_subtasks / total_subtasks"
      }
    },
    "orchestration": {
      "orchestration_accuracy": {
        "description": "How often the system selects the optimal orchestration pattern",
        "formula": "correct_pattern_selections / total_subtasks"
      },
      "adaptation_rate": {
        "description": "Success rate of mid-execution adaptations",
        "formula": "successful_adaptations / adaptation_attempts"
      },
      "escalation_efficiency": {
        "description": "How often escalations lead to success",
        "formula": "successful_escalations / total_escalations"
      }
    },
    "memory": {
      "memory_utilization": {
        "description": "Fraction of retrieved memories that are actually used",
        "formula": "used_memories / retrieved_memories"
      },
      "consolidation_rate": {
        "description": "Fraction of episodic memories promoted to semantic",
        "formula": "consolidated_entries / total_episodic_entries"
      }
    },
    "efficiency": {
      "cost_efficiency": {
        "description": "Success rate normalized by API cost",
        "formula": "success_rate / average_cost"
      },
      "trajectory_length": {
        "description": "Average number of steps to complete a task",
        "formula": "total_steps / completed_tasks"
      },
      "latency": {
        "description": "Average time to complete a task",
        "unit": "seconds"
      }
    }
  },
  "tool_pool": [
    "web_search",
