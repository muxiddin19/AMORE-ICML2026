    "memory_retrieval"
  ],
  "orchestration_patterns": {
    "single": {
      "description": "Single agent handles the subtask independently",
      "typical_agents": 1,
      "relative_cost": 1.0,
      "relative_latency": 1.0,
      "best_for": ["simple tasks", "clear requirements", "no parallelization benefit"]
    },
    "parallel": {
      "description": "Multiple agents work independently on different aspects",
      "typical_agents": "2-3",
      "relative_cost": 2.0,
      "relative_latency": 1.0,
      "best_for": ["data gathering", "independent subtask components", "coverage tasks"]
    },
    "hierarchical": {
      "description": "Supervisor delegates to and coordinates specialized workers",
      "typical_agents": "3-5",
      "relative_cost": "3-5x",
      "relative_latency": 2.0,
      "best_for": ["complex coordination", "quality review", "multi-step processes"]
    },
    "consensus": {
      "description": "Multiple agents deliberate and vote to reach agreement",
      "typical_agents": "3+",
      "relative_cost": "5-10x",
      "relative_latency": 3.0,
      "best_for": ["critical decisions", "high uncertainty", "multiple valid perspectives"]
    }
  },
  "annotation_guidelines": {
    "complexity_levels": {
      "low": {
        "score_range": [0.0, 0.3],
        "characteristics": ["single step", "clear inputs/outputs", "no ambiguity"],
        "typical_pattern": "single"
      },
      "medium_low": {
        "score_range": [0.3, 0.45],
        "characteristics": ["few steps", "some tool use", "minimal coordination"],
        "typical_pattern": "single or parallel"
      },
      "medium": {
        "score_range": [0.45, 0.6],
        "characteristics": ["multiple steps", "tool diversity", "some dependencies"],
        "typical_pattern": "parallel or hierarchical"
      },
      "medium_high": {
        "score_range": [0.6, 0.75],
        "characteristics": ["many steps", "complex dependencies", "expert knowledge"],
        "typical_pattern": "hierarchical"
      },
      "high": {
        "score_range": [0.75, 1.0],
        "characteristics": ["very complex", "high uncertainty", "synthesis required"],
        "typical_pattern": "hierarchical or consensus"
      }
    },
    "inter_annotator_agreement": {
      "metric": "Cohen's kappa",
      "threshold": 0.75,
      "achieved": 0.78
    }
  },
